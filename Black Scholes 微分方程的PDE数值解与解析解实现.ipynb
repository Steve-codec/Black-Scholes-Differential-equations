    "        ttk.Label(input_frame, text=\"期权类型:\").grid(row=2, column=2, sticky=tk.W, pady=5)\n",
    "        self.option_type_var = tk.StringVar(value=\"call\")\n",
    "        ttk.Radiobutton(input_frame, text=\"看涨期权\", variable=self.option_type_var, value=\"call\").grid(row=2, column=3, sticky=tk.W, pady=5)\n",
    "        ttk.Radiobutton(input_frame, text=\"看跌期权\", variable=self.option_type_var, value=\"put\").grid(row=2, column=4, sticky=tk.W, pady=5)\n",
    "        \n",
    "        # 计算按钮\n",
    "        ttk.Button(input_frame, text=\"计算期权价格\", command=self.calculate_option_price).grid(row=3, column=0, columnspan=5, pady=10)\n",
    "        \n",
    "        # 创建结果框架\n",
    "        result_frame = ttk.LabelFrame(root, text=\"计算结果\", padding=\"10\")\n",
    "        result_frame.pack(fill=tk.BOTH, expand=True, padx=10, pady=5)\n",
    "        \n",
    "        # 结果文本区域\n",
    "        self.result_text = tk.Text(result_frame, height=10, width=80, font=self.get_system_font())\n",
    "        self.result_text.pack(fill=tk.BOTH, expand=True, pady=5)\n",
    "        \n",
    "        # 创建图表框架\n",
    "        chart_frame = ttk.LabelFrame(root, text=\"价格敏感性分析\", padding=\"10\")\n",
    "        chart_frame.pack(fill=tk.BOTH, expand=True, padx=10, pady=5)\n",
    "        \n",
    "        # 创建图表\n",
    "        self.fig, self.ax = plt.subplots(figsize=(7, 4))\n",
    "        self.canvas = FigureCanvasTkAgg(self.fig, master=chart_frame)\n",
    "        self.canvas.get_tk_widget().pack(fill=tk.BOTH, expand=True)\n",
    "        \n",
    "        # 状态栏\n",
    "        self.status_var = tk.StringVar(value=\"就绪\")\n",
    "        ttk.Label(root, textvariable=self.status_var, relief=tk.SUNKEN, anchor=tk.W).pack(side=tk.BOTTOM, fill=tk.X)\n",
    "    \n",
    "    def configure_fonts(self):\n",
    "        \"\"\"配置 matplotlib 中文字体，使用系统实际存在的字体\"\"\"\n",
    "        # Windows 系统常用字体\n",
    "        windows_fonts = [\"SimHei\", \"Microsoft YaHei\", \"SimSun\", \"KaiTi\"]\n",
    "        # macOS 系统常用字体\n",
    "        mac_fonts = [\"Heiti TC\", \"PingFang TC\", \"STHeiti\"]\n",
    "        # Linux 系统常用字体\n",
    "        linux_fonts = [\"WenQuanYi Micro Hei\", \"Noto Sans CJK SC\"]\n",
    "        \n",
    "        # 检测系统并设置合适的字体\n",
    "        import platform\n",
    "        system = platform.system()\n",
    "        \n",
    "        if system == \"Windows\":\n",
    "            plt.rcParams[\"font.family\"] = windows_fonts\n",
    "        elif system == \"Darwin\":  # macOS\n",
    "            plt.rcParams[\"font.family\"] = mac_fonts\n",
    "        else:  # Linux\n",
    "            plt.rcParams[\"font.family\"] = linux_fonts\n",
    "        \n",
    "        plt.rcParams[\"axes.unicode_minus\"] = False  # 正确显示负号\n",
    "    \n",
    "    def get_system_font(self):\n",
    "        \"\"\"获取系统默认中文字体\"\"\"\n",
    "        import platform\n",
    "        system = platform.system()\n",
    "        \n",
    "        if system == \"Windows\":\n",
    "            return (\"SimHei\", 10)\n",
    "        elif system == \"Darwin\":  # macOS\n",
    "            return (\"Heiti TC\", 10)\n",
    "        else:  # Linux\n",
    "            return (\"WenQuanYi Micro Hei\", 10)\n",
    "    \n",
    "    def calculate_option_price(self):\n",
    "        \"\"\"计算期权价格并显示结果\"\"\"\n",
    "        try:\n",
    "            # 获取输入参数\n",
    "            S = self.S_var.get()\n",
    "            K = self.K_var.get()\n",
    "            T = self.T_var.get()\n",
    "            r = self.r_var.get() / 100  # 转换为小数\n",
    "            sigma = self.sigma_var.get() / 100  # 转换为小数\n",
    "            option_type = self.option_type_var.get()\n",
    "            \n",
    "            # 验证输入\n",
    "            if S <= 0 or K <= 0 or T <= 0 or r < 0 or sigma <= 0:\n",
    "                messagebox.showerror(\"输入错误\", \"所有数值必须为正数，利率必须非负\")\n",
    "                return\n",
    "            \n",
    "            # 计算期权价格\n",
    "            option_price, d1, d2 = self.solve_black_scholes_pde(S, K, T, r, sigma, option_type)\n",
    "            \n",
    "            # 显示结果\n",
    "            self.result_text.delete(1.0, tk.END)\n",
    "            self.result_text.insert(tk.END, f\"===== Black-Scholes 期权定价结果 =====\\n\\n\")\n",
    "            self.result_text.insert(tk.END, f\"标的资产价格 (S): {S:.2f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"行权价格 (K): {K:.2f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"到期时间 (T): {T:.2f} 年\\n\")\n",
    "            self.result_text.insert(tk.END, f\"无风险利率 (r): {r*100:.2f}%\\n\")\n",
    "            self.result_text.insert(tk.END, f\"波动率 (σ): {sigma*100:.2f}%\\n\\n\")\n",
    "            self.result_text.insert(tk.END, f\"期权类型: {'看涨期权' if option_type == 'call' else '看跌期权'}\\n\\n\")\n",
    "            self.result_text.insert(tk.END, f\"期权价格: {option_price:.6f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"d1: {d1:.6f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"d2: {d2:.6f}\\n\")\n",
    "            \n",
    "            # 计算希腊字母\n",
    "            delta = norm.cdf(d1) if option_type == 'call' else -norm.cdf(-d1)\n",
    "            gamma = np.exp(-d1**2/2) / (S * sigma * np.sqrt(2*np.pi*T))\n",
    "            theta = -S * sigma * np.exp(-d1**2/2) / (2 * np.sqrt(2*np.pi*T)) - r * K * np.exp(-r*T) * norm.cdf(d2) if option_type == 'call' else \\\n",
    "                    -S * sigma * np.exp(-d1**2/2) / (2 * np.sqrt(2*np.pi*T)) + r * K * np.exp(-r*T) * norm.cdf(-d2)\n",
    "            vega = S * np.sqrt(T) * np.exp(-d1**2/2) / np.sqrt(2*np.pi)\n",
    "            rho = K * T * np.exp(-r*T) * norm.cdf(d2) if option_type == 'call' else -K * T * np.exp(-r*T) * norm.cdf(-d2)\n",
    "            \n",
    "            self.result_text.insert(tk.END, f\"\\n===== 希腊字母 =====\\n\")\n",
    "            self.result_text.insert(tk.END, f\"Delta: {delta:.6f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"Gamma: {gamma:.6f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"Theta: {theta:.6f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"Vega: {vega:.6f}\\n\")\n",
    "            self.result_text.insert(tk.END, f\"Rho: {rho:.6f}\\n\")\n",
    "            \n",
    "            # 更新图表\n",
    "            self.update_chart(S, K, T, r, sigma, option_type)\n",
    "            \n",
    "            self.status_var.set(\"计算完成\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            messagebox.showerror(\"计算错误\", f\"计算过程中发生错误: {str(e)}\")\n",
    "            self.status_var.set(\"计算失败\")\n",
    "    \n",
    "    def solve_black_scholes_pde(self, S, K, T, r, sigma, option_type='call'):\n",
    "        \"\"\"从 Black-Scholes 偏微分方程直接推导并求解\"\"\"\n",
    "        # 1. 变量替换\n",
    "        x = np.log(S / K)  # 对数价格比\n",
    "        tau = sigma**2 * T  # 时间缩放\n",
    "        \n",
    "        # 2. 计算 d1 和 d2\n",
    "        d1 = (x + (r + 0.5 * sigma**2) * T) / (sigma * np.sqrt(T))\n",
    "        d2 = d1 - sigma * np.sqrt(T)\n",
    "        \n",
    "        # 3. 定义边界条件函数（到期时的期权价值）\n",
    "        def boundary_condition(y):\n",
    "            # y 是对数价格，对应 S' = K * e^y\n",
    "            S_prime = K * np.exp(y)\n",
    "            \n",
    "            if option_type.lower() == 'call':\n",
    "                # 看涨期权边界条件: max(S'-K, 0)\n",
    "                return max(S_prime - K, 0)\n",
    "            else:\n",
    "                # 看跌期权边界条件: max(K-S', 0)\n",
    "                return max(K - S_prime, 0)\n",
    "        \n",
    "        # 4. 计算期权价格（通过积分求解）\n",
    "        def integrand(z):\n", # z来自于数值积分算法的自动采样\n",
    "            # 几何布朗运动在 T 时刻的对数价格\n",
    "            y = x + (r - 0.5 * sigma**2) * T + sigma * np.sqrt(T) * z\n",
    "            \n",
    "            # 期权收益（应用边界条件）\n",
    "            payoff = boundary_condition(y)\n",
    "            \n",
    "            # 标准正态分布的概率密度\n",
    "            pdf = np.exp(-0.5 * z**2) / np.sqrt(2 * np.pi)\n",
    "            \n",
    "            return payoff * pdf\n",
    "        \n",
    "        # 5. 基于边界条件优化积分区间\n",
    "        lower_bound = -5  # 覆盖大部分概率质量\n",
    "        upper_bound = 5   # 覆盖大部分概率质量\n",
    "        \n",
    "        # 6. 计算期望收益并折现\n",
    "        expected_payoff, _ = quad(integrand, lower_bound, upper_bound, epsabs=1e-10, epsrel=1e-10)\n",
    "        option_price = np.exp(-r * T) * expected_payoff\n",
    "        \n",
    "        return option_price, d1, d2\n",
    "    \n",
    "    def update_chart(self, S, K, T, r, sigma, option_type):\n",
    "        \"\"\"更新价格敏感性分析图表\"\"\"\n",
    "        self.ax.clear()\n",
    "        \n",
    "        # 生成标的资产价格范围\n",
    "        S_range = np.linspace(0.5*S, 1.5*S, 100)\n",
    "        \n",
    "        # 计算不同价格下的期权价值\n",
    "        option_values = []\n",
    "        for s in S_range:\n",
    "            price, _, _ = self.solve_black_scholes_pde(s, K, T, r, sigma, option_type)\n",
    "            option_values.append(price)\n",
    "            \n",
    "        # 绘制期权价值曲线\n",
    "        self.ax.plot(S_range, option_values, 'b-', linewidth=2)\n",
    "        self.ax.set_title(f\"{'看涨' if option_type == 'call' else '看跌'}期权价值与标的资产价格关系\")\n",
    "        self.ax.set_xlabel(\"标的资产价格\")\n",
    "        self.ax.set_ylabel(\"期权价值\")\n",
    "        self.ax.grid(True)\n",
    "        \n",
    "        # 添加当前价格标记\n",
    "        current_price, _, _ = self.solve_black_scholes_pde(S, K, T, r, sigma, option_type)\n",
    "        self.ax.plot(S, current_price, 'ro', markersize=8)\n",
    "        self.ax.annotate(f'当前价格: {current_price:.2f}', \n",
    "                         xy=(S, current_price), \n",
    "                         xytext=(S+5, current_price+5),\n",
    "                         arrowprops=dict(facecolor='black', shrink=0.05))\n",
    "        \n",
    "        # 添加行权价格标记\n",
    "        self.ax.axvline(x=K, color='g', linestyle='--', label=f'行权价格: {K}')\n",
    "        self.ax.legend()\n",
    "        \n",
    "        self.canvas.draw()\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    root = tk.Tk()\n",
    "    app = OptionPricingCalculator(root)\n",
    "    root.mainloop()"
   ]
