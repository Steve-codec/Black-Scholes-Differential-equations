    "import numpy as np\n",
    "from scipy.stats import norm\n",
    "\n",
    "def black_scholes(S, K, T, r, sigma, option_type='call'):\n",
    "    \"\"\"\n",
    "    使用 Black-Scholes 模型计算欧式期权价格\n",
    "    \n",
    "    参数:\n",
    "    S: 标的资产当前价格\n",
    "    K: 行权价格\n",
    "    T: 到期时间（年）\n",
    "    r: 无风险利率\n",
    "    sigma: 标的资产价格波动率\n",
    "    option_type: 期权类型，'call' 为看涨期权，'put' 为看跌期权\n",
    "    \n",
    "    返回:\n",
    "    option_price: 期权价格\n",
    "    d1: Black-Scholes 公式中的中间变量\n",
    "    d2: Black-Scholes 公式中的中间变量\n",
    "    \"\"\"\n",
    "    # 计算中间变量 d1 和 d2\n",
    "    d1 = (np.log(S / K) + (r + 0.5 * sigma ** 2) * T) / (sigma * np.sqrt(T))\n",
    "    d2 = d1 - sigma * np.sqrt(T)\n",
    "    \n",
    "    # 根据期权类型计算价格\n",
    "    if option_type.lower() == 'call':\n",
    "        option_price = S * norm.cdf(d1) - K * np.exp(-r * T) * norm.cdf(d2)\n",
    "    elif option_type.lower() == 'put':\n",
    "        option_price = K * np.exp(-r * T) * norm.cdf(-d2) - S * norm.cdf(-d1)\n",
    "    else:\n",
    "        raise ValueError(\"期权类型必须是 'call' 或 'put'\")\n",
    "    \n",
    "    return option_price, d1, d2\n",
    "\n",
    "def get_user_input():\n",
    "    \"\"\"获取用户输入的期权参数\"\"\"\n",
    "    print(\"\\n===== Black-Scholes 期权定价模型 =====\")\n",
    "    \n",
    "    # 获取标的资产价格\n",
    "    while True:\n",
    "        try:\n",
    "            S = float(input(\"请输入标的资产当前价格 (例如 100): \"))\n",
    "            if S <= 0:\n",
    "                print(\"价格必须为正数，请重新输入。\")\n",
    "                continue\n",
    "            break\n",
    "        except ValueError:\n",
    "            print(\"输入无效，请输入一个数字。\")\n",
    "    \n",
    "    # 获取行权价格\n",
    "    while True:\n",
    "        try:\n",
    "            K = float(input(\"请输入行权价格 (例如 100): \"))\n",
    "            if K <= 0:\n",
    "                print(\"价格必须为正数，请重新输入。\")\n",
    "                continue\n",
    "            break\n",
    "        except ValueError:\n",
    "            print(\"输入无效，请输入一个数字。\")\n",
    "    \n",
    "    # 获取到期时间（年）\n",
    "    while True:\n",
    "        try:\n",
    "            T = float(input(\"请输入到期时间（年） (例如 1.0): \"))\n",
    "            if T <= 0:\n",
    "                print(\"时间必须为正数，请重新输入。\")\n",
    "                continue\n",
    "            break\n",
    "        except ValueError:\n",
    "            print(\"输入无效，请输入一个数字。\")\n",
    "    \n",
    "    # 获取无风险利率\n",
    "    while True:\n",
    "        try:\n",
    "            r_input = input(\"请输入无风险利率 (例如 0.05 表示 5%): \")\n",
    "            r = float(r_input)\n",
    "            if r < 0:\n",
    "                print(\"利率不能为负数，请重新输入。\")\n",
    "                continue\n",
    "            break\n",
    "        except ValueError:\n",
    "            print(\"输入无效，请输入一个数字。\")\n",
    "    \n",
    "    # 获取波动率\n",
    "    while True:\n",
    "        try:\n",
    "            sigma_input = input(\"请输入波动率 (例如 0.2 表示 20%): \")\n",
    "            sigma = float(sigma_input)\n",
    "            if sigma <= 0:\n",
    "                print(\"波动率必须为正数，请重新输入。\")\n",
    "                continue\n",
    "            break\n",
    "        except ValueError:\n",
    "            print(\"输入无效，请输入一个数字。\")\n",
    "    \n",
    "    # 获取期权类型\n",
    "    while True:\n",
    "        option_type = input(\"请输入期权类型 (call/put): \").lower()\n",
    "        if option_type in ['call', 'put']:\n",
    "            break\n",
    "        else:\n",
    "            print(\"无效的期权类型，请输入 'call' 或 'put'。\")\n",
    "    \n",
    "    return S, K, T, r, sigma, option_type\n",
    "\n",
    "def main():\n",
    "    # 获取用户输入\n",
    "    S, K, T, r, sigma, option_type = get_user_input()\n",
    "    \n",
    "    # 计算期权价格\n",
    "    option_price, d1, d2 = black_scholes(S, K, T, r, sigma, option_type)\n",
    "    \n",
    "    # 显示结果\n",
    "    print(\"\\n===== 计算结果 =====\")\n",
    "    print(f\"期权类型: {'看涨' if option_type == 'call' else '看跌'}期权\")\n",
    "    print(f\"标的资产价格: {S}\")\n",
    "    print(f\"行权价格: {K}\")\n",
    "    print(f\"到期时间: {T} 年\")\n",
    "    print(f\"无风险利率: {r*100:.2f}%\")\n",
    "    print(f\"波动率: {sigma*100:.2f}%\")\n",
    "    print(\"\\n计算结果:\")\n",
    "    print(f\"{option_type}期权价格: {option_price:.4f}\")\n",
    "    print(f\"d1 = {d1:.6f}\")\n",
    "    print(f\"d2 = {d2:.6f}\")\n",
    "    \n",
    "    # 如果用户同时想查看另一种期权价格\n",
    "    other_option = 'put' if option_type == 'call' else 'call'\n",
    "    other_price, _, _ = black_scholes(S, K, T, r, sigma, other_option)\n",
    "    print(f\"\\n{other_option}期权价格: {other_price:.4f}\")\n",
    "    \n",
    "    # 验证期权平价关系\n",
    "    if option_type == 'call':\n",
    "        put_call_parity = option_price - other_price - S + K * np.exp(-r * T)\n",
    "    else:\n",
    "        put_call_parity = other_price - option_price - S + K * np.exp(-r * T)\n",
    "    print(f\"\\n期权平价关系验证: {put_call_parity:.6f} (理论上应为0)\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
